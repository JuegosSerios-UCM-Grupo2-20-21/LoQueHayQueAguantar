  <graph-conversation id="ConversacionEntrada">
    <dialogue-node nodeindex="0" editor-x="99" editor-y="46">
      <speak-char idTarget="HombreEntrada">
        <resources name="No name" />
        <text>¡Menos mal! Solo con verte, me has alegrado el día.</text>
      </speak-char>
      <condition />
      <speak-char idTarget="HombreEntrada">
        <resources name="No name" />
        <text>Te invito a tomar algo.</text>
      </speak-char>
      <condition />
      <child nodeindex="1" />
    </dialogue-node>
    <option-node nodeindex="1" keepShowing="yes" preListening="yes" editor-x="112" editor-y="265" x="112" y="265">
      <speak-player>
        <text>¿Me dices a mí? Porque espero que no.</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="2" />
      <speak-player>
        <text>Espero que aquí la gente sea medio normal...</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="2" />
      <speak-player>
        <text>&lt;&lt;Prefiero ignorarle y entrar. No quiero llegar tarde&gt;&gt;</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="3" />
      <effect>
        <activate flag="TioHablado" />
        <condition />
      </effect>
    </option-node>
    <dialogue-node nodeindex="2" editor-x="687" editor-y="39">
      <speak-char idTarget="HombreEntrada">
        <resources name="No name" />
        <text>Solo intentaba ser amable, tampoco es para tanto.</text>
      </speak-char>
      <condition />
      <end-conversation />
    </dialogue-node>
    <dialogue-node nodeindex="3" editor-x="688" editor-y="230">
      <end-conversation>
        <effect>
          <trigger-scene idTarget="Hall" x="375" y="421" transitionTime="1" transitionType="4" />
          <condition />
        </effect>
      </end-conversation>
    </dialogue-node>
  </graph-conversation>
  <graph-conversation id="BucleEntrada">
    <dialogue-node nodeindex="0" editor-x="50" editor-y="50">
      <speak-char idTarget="HombreEntrada">
        <resources name="No name" />
        <text>Solo intentaba ser amable, tampoco es para tanto.</text>

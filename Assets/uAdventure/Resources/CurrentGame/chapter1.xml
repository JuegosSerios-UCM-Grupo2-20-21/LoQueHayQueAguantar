  <graph-conversation id="SalaEsperaBorde">
    <dialogue-node nodeindex="0" editor-x="37" editor-y="16">
      <speak-char idTarget="HombreBorde">
        <resources name="No name" />
        <text>Hola, soy Juan. ¿qué tal?</text>
      </speak-char>
      <condition />
      <speak-player>
        <resources name="No name" />
        <text>Pues de los nervios. Espero conseguir el puesto.</text>
      </speak-player>
      <condition />
      <speak-char idTarget="HombreBorde">
        <resources name="No name" />
        <text>Ah, ¿pero tú vienes a la entrevista?</text>
      </speak-char>
      <condition />
      <child nodeindex="1" />
      <effect>
        <activate flag="TioBordeHablado" />
        <condition />
      </effect>
    </dialogue-node>
    <dialogue-node nodeindex="1" editor-x="29" editor-y="295">
      <speak-player>
        <resources name="No name" />
        <text>&lt;&lt;Vaya, le ha cambiado el gesto. Ya no sonríe. ¿Se ha puesto a la defensiva?&gt;&gt;</text>
      </speak-player>
      <condition />
      <child nodeindex="2" />
    </dialogue-node>
    <option-node nodeindex="2" question="No me apetece mucho hablarle. ¿Le contesto?" keepShowing="yes" preListening="yes" editor-x="363" editor-y="233" x="363" y="233">
      <speak-player>
        <text>&lt;&lt;Ignorarle.&gt;&gt;</text>
        <resources name="No name" />
      </speak-player>
      <condition />
